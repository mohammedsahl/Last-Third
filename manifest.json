{
  "manifest_version": 2,
  "name": "Last Third",
  "description": "Calculate the last third of the night for your location",
  "version": "1.0.0",
  "icons": {"128": "icon_128.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions":["geolocation"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup":"popup.html",
    "default_title":"lastThird"
  }
}
